<div class="row mt">
  <div class="col-md-6 col-md-offset-3 content mas">
    <h1>Vote for the songs!</h1>
    <div class="pal">
      <%= link_to "Search Again!", songs_search_path %>

      <% @songs.each do |song| %>
      <div>
        <%= link_to fa_icon("arrow-circle-o-up"), vote_path(id: song.id ), params: { vote: '1'},method: :post %>
        <%= link_to fa_icon("arrow-circle-o-down"), vote_path(id: song.id ), params: { vote: '-1'} ,method: :post%>
        <%= song.title %>
        <%= song.artist %>
      </div>
      <% end %>
    </div>
  </div>
</div>
